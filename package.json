{
  "name": "s5-client-nodejs",
  "version": "0.5.0-beta2",
  "description": "s5-client for Decentralized content-addressed storage network S5",
  "repository": "https://github.com/parajbs/s5-client-nodejs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "format": "prettier --write .",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint . --max-warnings 0",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },  
  "keywords": [
    "Decentralised",
    "Content-Addressed",
    "Cloud Storage",
    "S5"
  ],
  "author": "parajbs",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "0.27.2",
    "form-data": "4.0.0",
    "mime": "^3.0.0",
    "url-join": "^4.0.1",
    "tweetnacl": "^1.0.3",
    "blake3": "^2.1.7",
    "tus-js-client": "^3.0.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.20.0",
    "axios-retry": "^3.2.5",
    "eslint": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.0.5",
    "cli-progress": "^3.11.2",
    "tmp": "0.2.1"
  }
}
